<Text-field prompt="&gt; " style="Maple Input" layout="Normal">num := proc(sys,x)::list;      #Get the number of occurrences of each variable in a single polynomial 
local i,len,m,n,f,number,degreesum;
number:=[];    #The first item indicates whether it exists, the second item indicates the number of occurrences (there are several                 single items), and the third item indicates the highest degree of the single item for a single argument (such as                    x1^2*x3+x1*x2, Should be a degree of x1^2*x3, the result is 3)
m:=0;    #The number of occurrences of x in f
n:=[];   #The weight of x in f
degreesum:=0;   #The sum of the degrees of x in f
len:=nops(sys);  #Number of items in f
for i from 1 to len do
   f:=[];
   f:=op(i,sys);
   if(degree(f,x)&lt;&gt;0) then
      m:=m+1;
      n:=[op(n),degree(f)];
      degreesum:=degreesum+degree(f,x);
   else
      n:=[op(n),0];
   end;
end;
if m&gt;0 then 
  number:=[op(number),1,m,max(n),degreesum];  
else
  number:=[op(number),0,m,max(n),degreesum];
end;
number;
end proc;

side:=proc(f1,f2)::list;       #Get the number of sides
local s,s1,s2,s3,n1,n2,i,j,k;
s:=[];
s1:=0;
s2:=0;
s3:=0;
n1:=[num(f1,x1)[1],num(f1,x2)[1],num(f1,x3)[1]];
n2:=[num(f2,x1)[1],num(f2,x2)[1],num(f2,x3)[1]];


if(n1[1]=1 and n2[1]=1) then
   if(n1[2]=1 or n2[2]=1) then
      s1:=s1+1;
   end:
   if(n1[3]=1 or n2[3]=1) then
       s1:=s1+1;
   end:
end:

if(n1[1]=1 and n2[1]=0) then
   s1:=n1[2]+n1[3];
end:
if(n1[1]=0 and n2[1]=1) then
     s1:=n2[2]+n2[3];
end;

if(n1[2]=1 and n2[2]=1) then
   if(n1[1]=1 or n2[1]=1) then
      s2:=s2+1;
   end:
   if(n1[3]=1 or n2[3]=1) then
